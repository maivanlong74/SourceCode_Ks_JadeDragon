@model IEnumerable<Jade_Dragon.Models.NguoiDung>

@{
    ViewBag.Title = "QuanLyKhManage";
    Layout = "~/Areas/Admin/Views/Shared/LayoutManage.cshtml";
}

<script src="~/Areas/Admin/Public/js/dataTables_min.js"></script>
<link href="~/Areas/Admin/Public/css/Table.css" rel="stylesheet" />

<div class="home">
    <div class="body">
        <table id="example" class="display">
            <thead>
                <tr>
                    <th>Tên khách hàng</th>
                    <th>Số điện thoại</th>
                    <th>CMND</th>
                    <th>Địa chỉ</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.HoTen</td>
                        <td>@item.SoDienThoai</td>
                        <td>@item.CMND</td>
                        <td>@item.DiaChi</td>
                    </tr>
                }
            </tbody>
            <tfoot>
                <tr>
                    <th>Tên khách hàng</th>
                    <th>Số điện thoại</th>
                    <th>CMND</th>
                    <th>Địa chỉ</th>
                </tr>
            </tfoot>
        </table>
    </div>
</div>
<script>
    $(document).ready(function () {
        $('#example').DataTable();
    });
</script>


